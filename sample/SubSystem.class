<?php
/*---------------------------------------------------------------------------*
 * Class - Entity Container
 *      サブシステム
 *---------------------------------------------------------------------------*/
namespace sample;

class SubSystem extends \PR\Container{

	protected $aNameTags = null;
	protected $aTargets = array('iSysId','sName','iDate');

	public function __construct($roDao=null){
		parent::__construct($roDao);
		$aPrimaryKeys = array('iSysId');
		$aInsKey = array('prCol_SysId'=>\PR\Model::INS_SERIAL);
		$sMyName = get_class($this);
		$this->setParameters('PRSubSystems',$aPrimaryKeys,$aInsKey,$sMyName);

		$this->aNameTags = array();
		$this->aNameTags['iSysId'] = new \PR\NameTag('iSysId','prReq_SysId','prCol_SysId','サブシステムID',\PR\Model::DT_NUMERIC);
		$this->aNameTags['sName'] = new \PR\NameTag('sName','prReq_SysName','s_prCol_SysName','サブシステム名',\PR\Model::DT_ALPHANUMERIC);
		$this->aNameTags['iDate'] = new \PR\NameTag('iDate','prReq_Date','prCol_Date','日時',\PR\Model::DT_DATE);
	}
}
?>
