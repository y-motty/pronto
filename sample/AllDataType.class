namespace sample;

class AllDataType extends \PR\Container{

	protected $aNameTags = null;
	protected $aTargets = array('iNumeric','fFloat','sAlphabet','sAlphaNumeric','sText','sPassword','sUrl','sEMail','sPostCode','sTel','iDate','sDomain');

	public function __construct($roDao=null){
		parent::__construct($roDao);
		$aPrimaryKeys = array('sAlphabet','iNumeric');
		$aInsKey = array('prCol_Numeric'=>\PR\Model::INS_SERIAL);
		$sMyName = get_class($this);
		$this->setParameters('PRAllDataTypes',$aPrimaryKeys,$aInsKey,$sMyName);

		$this->aNameTags = array();
		$this->aNameTags['iNumeric'] = new \PR\NameTag('iNumeric','prReq_Numeric','prCol_Numeric','数字A',\PR\Model::DT_NUMERIC);
		$this->aNameTags['fFloat'] = new \PR\NameTag('fFloat','prReq_Float','prCol_Float','小数A',\PR\Model::DT_FLOAT);
		$this->aNameTags['sAlphabet'] = new \PR\NameTag('sAlphabet','prReq_Alphabet','prCol_Alphabet','英字A',\PR\Model::DT_ALPHABET);
		$this->aNameTags['sAlphaNumeric'] = new \PR\NameTag('sAlphaNumeric','prReq_AlphaNumeric','s_prCol_AlphaNumeric','英数字A',\PR\Model::DT_ALPHANUMERIC);
		$this->aNameTags['sText'] = new \PR\NameTag('sText','prReq_Text','s_prCol_Text','テキストA',\PR\Model::DT_TEXT);
		$this->aNameTags['sPassword'] = new \PR\NameTag('sPassword','prReq_Password','s_prCol_Password','パスワードA',\PR\Model::DT_PASSWORD);
		$this->aNameTags['sUrl'] = new \PR\NameTag('sUrl','prReq_Url','s_prCol_Url','URL-A',\PR\Model::DT_URL);
		$this->aNameTags['sEMail'] = new \PR\NameTag('sEMail','prReq_EMail','prCol_EMail','メールアドレスA',\PR\Model::DT_EMAIL);
		$this->aNameTags['sPostCode'] = new \PR\NameTag('sPostCode','prReq_PostCode','prCol_PostCode','郵便番号A',\PR\Model::DT_POSTCODE);
		$this->aNameTags['sTel'] = new \PR\NameTag('sTel','prReq_Tel','s_prCol_Tel','電話番号A',\PR\Model::DT_TEL);
		$this->aNameTags['iDate'] = new \PR\NameTag('iDate','prReq_Date','prCol_Date','日時A',\PR\Model::DT_DATE);
		$this->aNameTags['sDomain'] = new \PR\NameTag('sDomain','prReq_Domain','prCol_Domain','ドメインA',\PR\Model::DT_DOMAIN);
	}
}
