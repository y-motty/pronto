namespace PR;

class NameTag{

	public $sVariable = null;
	public $sPost = null;
	public $sColumn = null;
	public $sTitle = null;
	public $iType = 0;

	function __construct($rsVariable=null,$rsPost=null,$rsColumn=null,$rsTitle=null,$riType=0){
		$this->sVariable = $rsVariable;
		$this->sPost = $rsPost;
		$this->sColumn = $rsColumn;
		$this->sTitle = $rsTitle;
		$this->iType = $riType;
	}
}
